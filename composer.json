{
  "name": "provider/steam",
  "description": "Steam data provider for Porter.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "connectors/http": "dev-5.0-rc",
    "connectors/ssl": "dev-5.0-rc",
    "eloquent/enumeration": "^5",
    "phpseclib/phpseclib": "^2",
    "scriptfusion/porter": "dev-data-source as 5",
    "symfony/css-selector": "^3|^4",
    "symfony/dom-crawler": "^3|^4"
  },
  "require-dev": {
    "athari/yalinqo": "^2.4.2",
    "phpunit/phpunit": "^6.5",
    "scriptfusion/phpunit-immediate-exception-printer": "^1",
    "mockery/mockery": "^1"
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Porter\\Provider\\Steam\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Porter\\Provider\\Steam\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit -c test --colors"
  },
  "config": {
    "platform": {
      "php": "7.1.99"
    },
    "sort-packages": true
  }
}
