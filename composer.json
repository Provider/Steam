{
  "name": "provider/steam",
  "description": "Steam data provider for Porter.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "scriptfusion/porter": "5.0.x-dev",
    "connectors/http": "5.0.x-dev",
    "connectors/ssl": "@dev",
    "symfony/dom-crawler": "^3",
    "symfony/css-selector": "^3",
    "eloquent/enumeration": "^5",
    "phpseclib/phpseclib": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "scriptfusion/phpunit-immediate-exception-printer": "^1",
    "mockery/mockery": "^1"
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Porter\\Provider\\Steam\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Porter\\Provider\\Steam\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit -c test"
  }
}
