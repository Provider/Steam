{
  "name": "provider/steam",
  "description": "Steam data provider for Porter.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "minimum-stability": "beta",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "connectors/http": "dev-master as 6",
    "eloquent/enumeration": "^5|^6",
    "phpseclib/phpseclib": "^2",
    "psr/log": "^1",
    "scriptfusion/porter": "dev-master as 7",
    "symfony/css-selector": "^5.3",
    "symfony/dom-crawler": "^5.4",
    "xpaw/steamid": "^2.0"
  },
  "require-dev": {
    "amphp/http": "^1.6@stable",
    "amphp/socket": "v2.0.0-beta.6",
    "mockery/mockery": "^1.3.3",
    "nikic/iter": "^2.2",
    "nunomaduro/collision": "^6.3",
    "phpunit/phpunit": "^9.5.23"
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Porter\\Provider\\Steam\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Porter\\Provider\\Steam\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit -c test --colors"
  },
  "config": {
    "sort-packages": true
  }
}
