{
  "name": "provider/steam",
  "description": "Steam data provider for Porter.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "amphp/parallel": "^1.1",
    "connectors/http": "dev-no-pool",
    "eloquent/enumeration": "^5|^6",
    "amphp/http-client": "v4.5.2",
    "phpseclib/phpseclib": "^2",
    "scriptfusion/porter": "^5",
    "symfony/css-selector": "^3|^4|^5",
    "symfony/dom-crawler": "^3|^4|^5"
  },
  "require-dev": {
    "amphp/phpunit-util": "^1.2",
    "athari/yalinqo": "^2.4.2",
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Porter\\Provider\\Steam\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Porter\\Provider\\Steam\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit -c test --colors"
  },
  "config": {
    "sort-packages": true
  }
}
